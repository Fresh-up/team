//冒泡法排序
function sortArr(arr) {
    for (var i = 0; i < arr.length - 1; i++) {//比较arr.length-1轮
        for (var j = i + 1; j < arr.length; j++) {
            if (arr[i] > arr[j]) {//交换
                var temp = arr[i];//临时变量
                arr[i] = arr[j];
                arr[j] = temp;

            }
        }
    }
    return arr;
}
